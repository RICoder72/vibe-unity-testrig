{
  "version": "1.0",
  "description": "Test scene creation with threading fix validation",
  "scene": {
    "name": "TestThreadingFix",
    "path": "Assets/Scenes",
    "type": "DefaultGameObjects",
    "create": true,
    "addToBuild": false
  },
  "commands": [
    {
      "action": "add-canvas",
      "name": "TestCanvas",
      "renderMode": "ScreenSpaceOverlay",
      "referenceWidth": 1920,
      "referenceHeight": 1080,
      "scaleMode": "ScaleWithScreenSize"
    },
    {
      "action": "add-button",
      "name": "TestButton",
      "parent": "TestCanvas",
      "text": "Threading Fix Test",
      "width": 200,
      "height": 50,
      "anchor": "MiddleCenter",
      "position": [0, 0]
    }
  ]
}